// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pessoas {
  id String @id
  nome String
  apelido String
  nascimento String
  stack String
  // stack Stack[]

}

